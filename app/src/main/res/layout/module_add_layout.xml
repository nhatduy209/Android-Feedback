<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <fragment
        android:id="@+id/nav_host_fragment"
        android:name="androidx.navigation.fragment.NavHostFragment"
        app:defaultNavHost="true"
        app:navGraph="@navigation/mobile_navigation"
        android:layout_height="0dp"
        android:layout_width="0dp"
        tools:ignore="MissingConstraints" />
    <TextView
        android:id="@+id/tvAddModule"
        android:layout_width="300dp"
        android:layout_height="51dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="4dp"
        android:text="Add Module List"
        android:textColor="@color/design_default_color_on_secondary"
        android:textSize="36sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:fontFamily="sans-serif-condensed-medium"
        tools:textAlignment="center" />

    <EditText
        android:id="@+id/txtAddModuleName"
        android:layout_width="368dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/edittext"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvAddModule" />

    <EditText
        android:id="@+id/txtAddModuleStartDate"
        android:layout_width="368dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/edittext"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtAddModuleName" />

    <EditText
        android:id="@+id/txtAddModuleEndDate"
        android:layout_width="368dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/edittext"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtAddModuleStartDate" />

    <EditText
        android:id="@+id/txtAddFBModuleTitle"
        android:layout_width="368dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/edittext"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtAddModuleAdmin" />

    <EditText
        android:id="@+id/txtAddFBModuleSD"
        android:layout_width="368dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/edittext"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtAddFBModuleTitle" />

    <EditText
        android:id="@+id/txtAddFBModuleED"
        android:layout_width="368dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/edittext"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtAddFBModuleSD" />

    <EditText
        android:id="@+id/txtAddModuleAdmin"
        android:layout_width="368dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/edittext"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtAddModuleEndDate" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="150dp"
        android:layout_height="22dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="48dp"
        android:text="Module Name"
        android:textColor="@color/design_default_color_on_secondary"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:fontFamily="serif" />

    <TextView
        android:id="@+id/txtTopicID"
        android:layout_width="366dp"
        android:layout_height="22dp"
        android:layout_marginStart="20dp"
        android:text="error"
        android:textColor="@android:color/holo_red_light"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtAddModuleName" />

    <TextView
        android:id="@+id/text11"
        android:layout_width="150dp"
        android:layout_height="22dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="18dp"
        android:text="Start Date"
        android:textColor="@color/design_default_color_on_secondary"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtAddModuleName" />

    <TextView
        android:id="@+id/txtTopicName"
        android:layout_width="366dp"
        android:layout_height="22dp"
        android:layout_marginStart="20dp"
        android:text="error"
        android:textColor="@android:color/holo_red_light"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtAddModuleStartDate" />

    <TextView
        android:id="@+id/text22"
        android:layout_width="150dp"
        android:layout_height="22dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="18dp"
        android:text="End Date"
        android:textColor="@color/design_default_color_on_secondary"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtAddModuleStartDate" />

    <TextView
        android:id="@+id/txtQuestionID"
        android:layout_width="366dp"
        android:layout_height="22dp"
        android:layout_marginStart="20dp"
        android:text="error"
        android:textColor="@android:color/holo_red_light"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtAddModuleEndDate" />

    <TextView
        android:id="@+id/text33"
        android:layout_width="150dp"
        android:layout_height="22dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="18dp"
        android:text="Admin ID"
        android:textColor="@color/design_default_color_on_secondary"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtAddModuleEndDate" />

    <TextView
        android:id="@+id/txtQuestionContent"
        android:layout_width="366dp"
        android:layout_height="22dp"
        android:layout_marginStart="20dp"
        android:text="error"
        android:textColor="@android:color/holo_red_light"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtAddModuleAdmin" />

    <TextView
        android:id="@+id/text44"
        android:layout_width="150dp"
        android:layout_height="22dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="18dp"
        android:text="Feedback Title"
        android:textColor="@color/design_default_color_on_secondary"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtAddModuleAdmin" />

    <TextView
        android:id="@+id/txtModuleEndDate"
        android:layout_width="366dp"
        android:layout_height="22dp"
        android:layout_marginStart="20dp"
        android:text="error"
        android:textColor="@android:color/holo_red_light"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtAddFBModuleTitle" />

    <TextView
        android:id="@+id/text55"
        android:layout_width="213dp"
        android:layout_height="23dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="18dp"
        android:text="Feedback Start Date"
        android:textColor="@color/design_default_color_on_secondary"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtAddFBModuleTitle" />

    <TextView
        android:id="@+id/txtModuleFBTitle"
        android:layout_width="366dp"
        android:layout_height="22dp"
        android:layout_marginStart="20dp"
        android:text="error"
        android:textColor="@android:color/holo_red_light"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtAddFBModuleSD" />

    <TextView
        android:id="@+id/text66"
        android:layout_width="213dp"
        android:layout_height="22dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="18dp"
        android:text="Feedback End Date"
        android:textColor="@color/design_default_color_on_secondary"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtAddFBModuleSD" />

    <TextView
        android:id="@+id/txtModuleFBStartDate"
        android:layout_width="366dp"
        android:layout_height="22dp"
        android:layout_marginStart="20dp"
        android:text="error"
        android:textColor="@android:color/holo_red_light"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtAddFBModuleED" />

    <Button
        android:id="@+id/btnSaveAddModule"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_marginStart="202dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/btn_save"
        android:fontFamily="serif"
        android:text="Save"
        android:textAllCaps="false"
        android:textColor="@color/design_default_color_background"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtModuleFBStartDate" />

    <Button
        android:id="@+id/btnBackAddModule"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/btn_back"
        android:text="Back"
        android:textAllCaps="false"
        android:textColor="@color/design_default_color_background"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/btnSaveAddModule"
        app:layout_constraintTop_toBottomOf="@+id/txtModuleFBStartDate" />

</androidx.constraintlayout.widget.ConstraintLayout>